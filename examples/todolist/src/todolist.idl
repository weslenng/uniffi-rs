namespace todolist {
};

dictionary TodoEntry {
    string text;
};

[Error]
enum AddItemError {
    "NetworkError", "TypeError",
};

[Error]
enum GetLastError {
    "NoTodoError", "UnknownError"
};

interface TodoList {
    constructor();
    [Throws=AddItemError]
    void add_item(string todo);
    void add_entry(TodoEntry entry);
    TodoEntry get_last_entry();
    [Throws=GetLastError]
    string get_last();
};
